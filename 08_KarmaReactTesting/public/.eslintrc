{
    "extends": "../.eslintrc",
    "parserOptions": {
        "ecmaFeatures": {
            "defaultParams": true,
            "destructuring": true,
            "forOf": true,
            "generators": false,
            "jsx": true,
            "modules": false
        },
        "sourceType": "script"
    },
    "plugins": [ "react" ],
    "rules": {
        "react/jsx-no-bind": 0,
        "react/jsx-indent-props": [2, 4],
        "react/jsx-indent": [2, 4]
    }
}